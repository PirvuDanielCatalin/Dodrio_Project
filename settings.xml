<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
   <mirrors>
       <mirror>
         <id>nexus-public</id>
         <mirrorOf>*</mirrorOf>
         <url>http://docker.lab.example.com:8081/nexus/content/repositories/public/</url>
       </mirror>
       <mirror>
         <id>nexus-snapshots</id>
         <mirrorOf>*</mirrorOf>
         <url>http://docker.lab.example.com:8081/nexus/content/repositories/snapshots/</url>
       </mirror>
       <mirror>
         <id>nexus-releases</id>
         <mirrorOf>*</mirrorOf>
         <url>http://docker.lab.example.com:8081/nexus/content/repositories/releases/</url>
       </mirror>
   </mirrors>
   <servers>
     <server>
       <id>nexus-public</id>
       <username>admin</username>
       <password>admin123</password>
     </server>
     <server>
       <id>nexus-snapshots</id>
       <username>admin</username>
       <password>admin123</password>
     </server>
     <server>
       <id>nexus-releases</id>
       <username>admin</username>
       <password>admin123</password>
     </server>
  </servers>
</settings>

